{
    "name": "@nuxtjs-alt/auth",
    "version": "2.0.4",
    "description": "An alternative module to @nuxtjs/auth",
    "homepage": "https://github.com/Teranode/nuxt-module-alternatives",
    "author": "Teranode",
    "keywords": [
        "nuxt",
        "nuxtjs",
        "nuxt-module",
        "nuxt-plugin",
        "nuxt-module-alternatives",
        "@nuxtjs/auth"
    ],
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
        "prepack": "npx unbuild"
    },
    "dependencies": {
        "@nuxt/kit": "^3.0.0-rc.8",
        "@nuxtjs-alt/http": "^1.2.6",
        "@pinia/nuxt": "^0.4.1",
        "body-parser": "latest",
        "cookie-es": "latest",
        "defu": "^6.0.0",
        "fs-extra": "latest",
        "jwt-decode": "latest",
        "ohash": "latest",
        "pathe": "latest",
        "requrl": "latest"
    },
    "devDependencies": {
        "@nuxt/module-builder": "latest",
        "@types/fs-extra": "latest",
        "eslint": "latest",
        "nuxt": "^3.0.0-rc.8"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Teranode/nuxt-module-alternatives.git",
        "directory": "@nuxtjs-alt/auth"
    },
    "bugs": {
        "url": "https://github.com/Teranode/nuxt-module-alternatives/issues"
    }
}
