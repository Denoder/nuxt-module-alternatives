{
    "name": "@nuxtjs-alt/axios",
    "version": "1.0.19",
    "description": "An alternative module to @nuxtjs/axios",
    "homepage": "https://github.com/Teranode/nuxt-module-alternatives",
    "author": "Teranode",
    "keywords": [
        "nuxt",
        "nuxtjs",
        "nuxt-module",
        "nuxt-plugin",
        "nuxt-module-alternatives",
        "axios",
        "@nuxtjs/axios"
    ],
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/module.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
        "prepack": "unbuild"
    },
    "dependencies": {
        "@nuxt/kit": "^3.0.0-rc.9",
        "axios": "^0.27.2",
        "axios-retry": "^3.3.1",
        "defu": "^6.1.0"
    },
    "devDependencies": {
        "@nuxt/module-builder": "^0.1.7",
        "@nuxt/schema": "3.0.0-rc.9",
        "@types/node": "^18.7.15",
        "nuxt": "^3.0.0-rc.9",
        "unbuild": "^0.8.10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Teranode/nuxt-module-alternatives.git",
        "directory": "@nuxtjs-alt/axios"
    },
    "bugs": {
        "url": "https://github.com/Teranode/nuxt-module-alternatives/issues"
    }
}
