{
    "name": "@nuxtjs-alt/http",
    "version": "1.5.0",
    "description": "An extended module to ohmyfetch",
    "homepage": "https://github.com/Teranode/nuxt-module-alternatives",
    "author": "Teranode",
    "keywords": [
        "nuxt",
        "nuxtjs",
        "nuxt-module",
        "nuxt-plugin",
        "nuxt-module-alternatives",
        "ohmyfetch",
        "@nuxtjs/http"
    ],
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "unbuild --stub && nuxi prepare playground",
        "prepack": "unbuild"
    },
    "dependencies": {
        "@nuxt/kit": "^3.0.0",
        "@refactorjs/ofetch": "latest",
        "defu": "latest",
        "ufo": "latest"
    },
    "devDependencies": {
        "nuxt": "^3.0.0",
        "unbuild": "latest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Teranode/nuxt-module-alternatives.git",
        "directory": "@nuxtjs-alt/http"
    },
    "bugs": {
        "url": "https://github.com/Teranode/nuxt-module-alternatives/issues"
    }
}
