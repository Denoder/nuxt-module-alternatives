{
    "name": "@nuxtjs-alt/http",
    "version": "1.4.1",
    "description": "An extended module to ohmyfetch",
    "homepage": "https://github.com/Teranode/nuxt-module-alternatives",
    "author": "Teranode",
    "keywords": [
        "nuxt",
        "nuxtjs",
        "nuxt-module",
        "nuxt-plugin",
        "nuxt-module-alternatives",
        "ohmyfetch",
        "@nuxtjs/http"
    ],
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
        "prepack": "unbuild"
    },
    "dependencies": {
        "@nuxt/kit": "^3.0.0-rc.12",
        "@refactorjs/ofetch": "^0.1.8",
        "defu": "^6.1.0",
        "ohash": "^0.1.5",
        "pathe": "^0.3.9"
    },
    "devDependencies": {
        "@nuxt/schema": "^3.0.0-rc.12",
        "@nuxt/module-builder": "^0.2.0",
        "@types/node": "^18.8.5",
        "fdir": "^5.2.0",
        "nuxt": "^3.0.0-rc.12",
        "unbuild": "^0.9.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Teranode/nuxt-module-alternatives.git",
        "directory": "@nuxtjs-alt/http"
    },
    "bugs": {
        "url": "https://github.com/Teranode/nuxt-module-alternatives/issues"
    }
}
